{
  "environments": {
    "review": {
      "scripts": {
        "postdeploy": "node ./postdeployReviewapp.js && pg_dump $STAGING_DATABASE_URL | psql $DATABASE_URL && bundle exec rake db:migrate"
      },
      "addons": [
        "heroku-postgresql:hobby-dev"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "env":{
    "HEROKU_APP_NAME": {
      "required": true
  },
    "HEROKU_API_TOKEN":{
      "required":true
    }
  }
}